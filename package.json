    "dirty-chai": "^2.0.1",
    "dts-css-modules-loader": "^1.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-config-appfolio-base": "^1.0.3",
    "eslint-import-resolver-webpack": "^0.11.1",
    "fetch-mock": "^7.3.3",
    "ignore-styles": "~5.0.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.4",
    "mocha-junit-reporter": "^1.17.0",
