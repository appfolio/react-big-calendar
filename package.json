{
  "name": "@appfolio/react-big-calendar-apm",
  "version": "0.20.3",
  "description": "Calendar! with events",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "repository": "intljusticemission/react-big-calendar",
  "license": "MIT",
  "main": "lib/index.js",
  "style": "lib/css/react-big-calendar.css",
  "publishConfig": {
    "registry": "https://nexus.dev.appf.io/repository/npm-hosted/"
  },
  "files": [
    "lib/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "calendar",
    "events",
    "full calendar"
  ],
  "scripts": {
    "dist:es": "babel ./src --extensions .js,.jsx --out-dir es --source-maps",
    "lint": "eslint src test",
    "prepublish": "npm run dist",
    "postpublish": "git push --no-verify && git push --tags",
    "start": "start-storybook -p 6006 -s ./docs/public",
    "build-storybook": "build-storybook",
    "docs": "BABEL_ENV=storybook build-storybook -o docs",
    "update-docs": "npm run docs && git add -A docs",
    "version": "npm run docs && git add -A docs",
    "l": "lessc --autoprefix=\"ie >= 10, last 2 versions\" ",
    "less": "npm run l src/less/styles.less ./lib/css/react-big-calendar.css && npm run less-dnd",
    "less-dnd": "npm run l src/addons/dragAndDrop/styles.less ./lib/addons/dragAndDrop/styles.css",
    "assets": "cpy src/less/* lib/less && npm run assets-addons",
    "assets-addons": "cpy addons/**/*.less ../lib/ --cwd=src --parents",
    "build": "npm run clean && babel src --out-dir lib && npm run assets && npm run less",
    "testA": "NODE_ENV=test nyc --reporter text --reporter lcov jest --coverage",
    "test": "NODE_ENV=test mocha  --require ./test/helpers.js 'test/**/*.js' --exit",
    "testold": "mocha --recursive --timeout 10000 --compilers js:babel-register -r test/setup.js test",
    "dist": "webpack -p && npm run dist:es",
    "clean": "rimraf lib"
  },
  "peerDependencies": {
    "@appfolio/react-gears": ">= 5.x",
    "react": "16.x",
    "react-dom": "16.x",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@appfolio/react-gears": "^5.19.2",
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.1",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-actions": "^5.3.14",
