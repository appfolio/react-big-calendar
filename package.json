    "chai-moment": "^0.1.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dev_server.js",
      "webpack.*config.js"
    ]
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "date-arithmetic": "^3.0.0",
    "dom-helpers": "^2.3.0 || ^3.0.0",
    "invariant": "^2.1.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "react-overlays": "^0.7.0",
    "react-prop-types": "^0.4.0",
    "uncontrollable": "^3.3.1 || ^4.0.0",
    "warning": "^2.0.0",
    "webpack-atoms": "^15.0.1"
  }
}
