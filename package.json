    "node-fetch": "^2.1.2",
    "nyc": "^12.0.2",
    "postcss": "^7.0.35",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "16.8",
    "sinon": "^9.2.1",
    "style-loader": "^2.0.0",
    "text-mask-addons": "^3.8.0",
